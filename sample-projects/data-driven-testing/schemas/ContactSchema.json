{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/contact-schema.json",
  "type": "object",
  "title": "The Contact Schema",
  "required": [
    "name",
    "email",
    "designation",
    "organization",
    "country"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Property",
      "default": "",
      "examples": [
        "John Doe"
      ],
      "pattern": "^(.*)$"
    },
    "email": {
      "$id": "#/properties/email",
      "type": "string",
      "title": "The Email Schema",
      "default": "",
      "examples": [
        "john.doe@example.com"
      ],
      "pattern": "^(.*)$"
    },
    "designation": {
      "$id": "#/properties/designation",
      "type": "string",
      "title": "The Designation Schema",
      "default": "",
      "examples": [
        "Chief Technical Officer"
      ],
      "pattern": "^(.*)$"
    },
    "organization": {
      "$id": "#/properties/organization",
      "type": "string",
      "title": "The Organization Schema",
      "default": "",
      "examples": [
        "example.com"
      ],
      "pattern": "^(.*)$"
    },
    "country": {
      "$id": "#/properties/country",
      "type": "string",
      "title": "The Country Schema",
      "default": "",
      "examples": [
        "India"
      ],
      "pattern": "^(.*)$"
    }
  }
}